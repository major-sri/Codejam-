T = int(input())
for x in range(1, T + 1):
    N, B = map(int, input().split())
    S = map(int, input().split())
    S = sorted(S) 
    c=0
    y=0
    for i in range(0,N):
        c+=S[i]
        if c<B:
            y+=1
        elif c==B:
            y+=1
            break
        else:
            break 
    print("Case #{}: {}".format(x, y), flush=True)
    


